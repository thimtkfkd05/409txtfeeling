{
  "manifest_version": 2,

  "name": "Text Feeling",
  "description": "This extension let users know what feelings are contained in text of the current page.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "views/popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "http://localhost:3000/*",
    "http://home.iacryl.com:7070/*"
  ]
}
